{
  "name": "oracle-arena",
  "version": "1.0.0",
  "description": "Oracle Arena - Supra Web3 Battle Game with Real StarKey Wallet Integration",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "python3 -m http.server 8000",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss style.css -o dist/style.min.css",
    "build:js": "webpack --mode=production",
    "deploy": "npm run build && npm run deploy:vercel",
    "deploy:vercel": "vercel --prod",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "supra",
    "web3",
    "blockchain",
    "gaming",
    "oracle",
    "vrf",
    "starkey",
    "ethereum"
  ],
  "author": "Oracle Arena Team",
  "license": "MIT",
  "dependencies": {
    "@starkey-labs/starkey-js": "^1.0.0",
    "ethers": "^6.8.1",
    "web3": "^4.2.2",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-preset-env": "^9.1.0",
    "prettier": "^3.0.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/oracle-arena.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/oracle-arena/issues"
  },
  "homepage": "https://oracle-arena.supra.com"
}
